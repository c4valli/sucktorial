{
    "manifest_version": 2,
    "name": "Factorial mocker",
    "version": "0.1",
  
    "description": "Allows to mock Factorial requests",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],

    "background": {
      "scripts": ["background.js"]
    }
}
  